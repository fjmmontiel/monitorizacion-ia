    "scripts":     {
        "clean": "run-s -l \"clean:dist\" \"clean:deps\"",
        "clean:deps": "lerna clean --yes && architecture-scripts deleteFolders --folders='node_modules'",
        "clean:dist": "architecture-scripts deleteFolders --folders='packages/**/coverage,packages/**/build'",
        "clean:cache": "npx nx reset",
        "test": "lerna run test",
        "lint": "lerna run lint",
        "lint:eslint": "lerna run lint:eslint",
        "lint:eslint:fix": "lerna run lint:eslint:fix",
        "typecheck": "lerna run typecheck",
        "build": "npm run clean:dist && lerna run build",
        "build:feature": "npm run clean:dist && lerna run build:feature",
        "build:development": "npm run clean:dist && lerna run build:development",
        "build:integration": "npm run clean:dist && lerna run build:integration",
        "build:preproduction": "npm run clean:dist && lerna run build:preproduction",
        "build:int_preproduction": "npm run clean:dist && lerna run build:int_preproduction",
        "build:production": "npm run clean:dist && lerna run build:production",
        "build:int_production": "npm run clean:dist && lerna run build:int_production",
        "start": "lerna run start --stream",
        "start:mock": "run-p -l -r \"start:mockoon\" \"start\"",
        "start:development": "lerna run start:development --stream",
        "start:integration": "lerna run start:integration --stream",
        "start:production": "lerna run start:production --stream",
        "prepare": "husky install",
        "start:mockoon": "mockoon-cli start --data mockoon.json --port 8080 --disable-log-to-file",
        "stop:mockoon": "mockoon-cli stop",
        "_cypress:functional": "lerna run cypress:functional",
        "cypress:functional": "run-p -l -r \"start:mockoon\" \"_cypress:functional\"",
        "_cypress:e2e": "lerna run cypress:e2e",
        "cypress:e2e": "start-server-and-test start:mock tcp:3000 _cypress:e2e",
        "cypress:report": "architecture-scripts deleteFolders --folders='report' && lerna run cypress:report",
        "complete-check": "run-s -l typecheck lint test build cypress:functional cypress:e2e",
        "predeploy": "architecture-scripts copyDistFiles --pattern='packages/**/build/*' --target='build' --distFolder='build/' --ignore='node_modules,bin,**/shared-library/**,**/cypress/**' --removeParentFolder='packages/' --rootPackage='shell'",
        "add-change": "changeset add",
        "create-version": "changeset version",
        "check-change": "changeset status",
        "deps-graph": "npx nx graph",
        "cz": "cz",
        "cz:retry": "cz --retry"
    },
    "engines":     {
        "npm": ">=8",
        "node": ">=18"
