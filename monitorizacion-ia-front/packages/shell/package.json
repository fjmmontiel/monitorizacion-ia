{
  "name": "@iagmvps-front-hipotecas/shell",
  "version": "0.0.1",
  "description": "Shell MFE application",
  "main": "./src/index.ts",
  "scripts": {
    "webpack": "webpack",
    "build": "dotenv -e .env.mock npm run webpack",
    "build:feature": "dotenv -e .env.feature npm run webpack",
    "build:development": "dotenv -e .env.development npm run webpack",
    "build:integration": "dotenv -e .env.integration npm run webpack -- -- --mode production",
    "build:preproduction": "dotenv -e .env.preproduction npm run webpack -- -- --mode production",
    "build:int_preproduction": "dotenv -e .env.int_preproduction npm run webpack -- -- --mode production",
    "build:production": "dotenv -e .env.production npm run webpack -- -- --mode=production",
    "build:int_production": "dotenv -e .env.int_production npm run webpack -- -- --mode production",
    "start": "dotenv -e .env.mock npm run webpack -- -- serve",
    "start:development": "dotenv -e .env.development npm run webpack -- -- serve",
    "start:integration": "dotenv -e .env.integration npm run webpack -- -- serve",
    "start:production": "dotenv -e .env.production npm run webpack -- -- serve --mode=production",
    "lint": "npm run lint:eslint",
    "lint:eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:eslint:fix": "npm run lint:eslint -- --fix",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "cypress:open": "dotenv -e .env.mock cypress open",
    "_cypress:functional": "cypress run --component",
    "cypress:functional": "dotenv -e .env.mock -- npm run _cypress:functional",
    "cypress:report": "npm run cypress:report:merge && npm run cypress:report:generate",
    "cypress:report_functional": "npm run cypress:report:merge && npm run cypress:report:generate",
    "cypress:report:merge": "mochawesome-merge ./cypress/report/*.json -o ./cypress/report/merged_reports.json",
    "cypress:report:generate": "marge ./cypress/report/merged_reports.json -f index -o ../../report/functional/shell --charts"
  },
  "devDependencies": {
    "@architecture-components/eslint-config": "0.1.0",
    "@architecture-components/scripts": "1.0.0",
    "@architecture-components/stylelint-config": "0.1.0",
    "@architecture-components/test-config": "0.3.0",
    "@architecture-components/typescript-config": "0.2.0",
    "@swc/core": "1.3.57",
    "@testing-library/cypress": "9.0.0",
    "cypress": "12.12.0",
    "dotenv-cli": "7.2.1",
    "eslint-plugin-cypress": "2.13.3",
    "zod": "3.21.4"
  },
  "dependencies": {
    "@architecture-components/channel-manager": "0.1.16",
    "@architecture-components/input": "2.1.0",
    "@architecture-components/link": "3.1.0",
    "@architecture-components/modal": "2.1.18",
    "@architecture-components/tab-bar": "2.1.0",
    "@architecture-components/text": "1.0.14",
    "@architecture-components/top-bar": "3.0.0",
    "@iagmvps-front-hipotecas/shared-library": "0.0.1",
    "@internal-channels-components/assets": "0.0.1",
    "@internal-channels-components/auth": "0.0.4",
    "@internal-channels-components/button": "0.1.0",
    "@internal-channels-components/error-handler": "0.0.1",
    "@internal-channels-components/http": "0.0.1",
    "@internal-channels-components/i18n": "0.0.1",
    "@internal-channels-components/icon": "0.0.1",
    "@internal-channels-components/import-helpers": "0.0.1",
    "@internal-channels-components/input-text": "0.0.1",
    "@internal-channels-components/storage": "0.0.1",
    "@internal-channels-components/textarea": "0.0.1",
    "@internal-channels-components/toast": "^0.0.1",
    "@svgr/webpack": "8.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.11.1"
  },
  "peerDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.10",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.7.3",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.5.1",
    "raw-loader": "4.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-refresh": "0.14.0",
    "start-server-and-test": "2.0.0",
    "style-loader": "3.3.2",
    "styled-components": "5.3.6",
    "terser-webpack-plugin": "5.3.8",
    "tsconfig-paths-webpack-plugin": "4.0.1",
    "url-loader": "4.1.1",
    "webpack": "5.75.0",
    "webpack-cli": "5.1.1",
    "webpack-dev-server": "4.15.0"
  }
}
